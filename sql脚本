CREATE TABLE `admin_company_info` (
  `id` bigint(20) NOT NULL AUTO_INCREMENT,
  `company` varchar(200) DEFAULT NULL COMMENT '公司名',
  `remark` varchar(200) DEFAULT NULL COMMENT '备注',
  `createBy` varchar(200) DEFAULT NULL COMMENT '创建人',
  `create_time` timestamp NULL DEFAULT CURRENT_TIMESTAMP COMMENT '创建时间',
  `updateBy` varchar(200) DEFAULT NULL COMMENT '更新人',
  `update_time` timestamp NULL DEFAULT CURRENT_TIMESTAMP COMMENT '更新时间',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=3 DEFAULT CHARSET=utf8 COMMENT='公司信息'


CREATE TABLE `admin_department_info` (
  `id` bigint(20) NOT NULL AUTO_INCREMENT,
  `department` varchar(200) DEFAULT NULL COMMENT '部门名称',
  `remark` varchar(200) DEFAULT NULL COMMENT '备注',
  `createBy` varchar(200) DEFAULT NULL COMMENT '创建人',
  `create_time` timestamp NULL DEFAULT CURRENT_TIMESTAMP COMMENT '创建时间',
  `updateBy` varchar(200) DEFAULT NULL COMMENT '更新人',
  `update_time` timestamp NULL DEFAULT CURRENT_TIMESTAMP COMMENT '更新时间',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=3 DEFAULT CHARSET=utf8 COMMENT='部门信息'




CREATE TABLE `admin_user_info` (
  `id` bigint(20) NOT NULL AUTO_INCREMENT,
  `name` varchar(200) DEFAULT NULL COMMENT '名字',
  `password` varchar(200) DEFAULT NULL COMMENT '密码',
  `company_id` varchar(200) DEFAULT NULL COMMENT '公司id',
  `department_id` varchar(200) DEFAULT NULL COMMENT '部门id',
  `remark` varchar(200) DEFAULT NULL COMMENT '备注',
  `createBy` varchar(200) DEFAULT NULL COMMENT '创建人',
  `create_time` varchar(200) DEFAULT NULL COMMENT '创建时间',
  `updateBy` varchar(200) DEFAULT NULL COMMENT '更新人',
  `update_time` varchar(200) DEFAULT NULL COMMENT '更新时间',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=6 DEFAULT CHARSET=utf8 COMMENT='部门信息'
